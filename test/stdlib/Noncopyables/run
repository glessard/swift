#/bin/bash

export DYLD_LIBRARY_PATH=/Codage/swift-public/build/Ninja-ReleaseAssert/swift-macosx-arm64/lib/swift/macosx

/Codage/swift-public/build/Ninja-ReleaseAssert/swift-macosx-arm64/bin/swift \
  -sdk '/Volumes/Xcode/Xcode-15A.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX14.2.sdk/' \
  -resource-dir /Codage/swift-public/build/Ninja-ReleaseAssert/swift-macosx-arm64/lib/swift \
  -enable-experimental-feature BuiltinModule \
  -enable-experimental-feature NoncopyableGenerics \
  -enable-experimental-feature NonescapableTypes \
  -Xfrontend -enable-experimental-associated-type-inference \
  -Xfrontend -enable-experimental-lifetime-dependence-inference \
  -Xfrontend -disable-experimental-parser-round-trip \
  "${1}"
